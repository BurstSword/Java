<?xml version="1.0" encoding="UTF-8"?>
<project name="module_optimizacionydocumentacion" default="compile.module.optimizacionydocumentacion">
  <dirname property="module.optimizacionydocumentacion.basedir" file="${ant.file.module_optimizacionydocumentacion}"/>
  
  <property name="module.jdk.home.optimizacionydocumentacion" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.optimizacionydocumentacion" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.optimizacionydocumentacion" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.optimizacionydocumentacion" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="optimizacionydocumentacion.output.dir" value="${module.optimizacionydocumentacion.basedir}/out/production/OptimizacionYDocumentacion"/>
  <property name="optimizacionydocumentacion.testoutput.dir" value="${module.optimizacionydocumentacion.basedir}/out/test/OptimizacionYDocumentacion"/>
  
  <path id="optimizacionydocumentacion.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="optimizacionydocumentacion.module.production.classpath">
    <path refid="${module.jdk.classpath.optimizacionydocumentacion}"/>
  </path>
  
  <path id="optimizacionydocumentacion.runtime.production.module.classpath">
    <pathelement location="${optimizacionydocumentacion.output.dir}"/>
  </path>
  
  <path id="optimizacionydocumentacion.module.classpath">
    <path refid="${module.jdk.classpath.optimizacionydocumentacion}"/>
    <pathelement location="${optimizacionydocumentacion.output.dir}"/>
  </path>
  
  <path id="optimizacionydocumentacion.runtime.module.classpath">
    <pathelement location="${optimizacionydocumentacion.testoutput.dir}"/>
    <pathelement location="${optimizacionydocumentacion.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.optimizacionydocumentacion">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.optimizacionydocumentacion">
    <patternset refid="excluded.from.module.optimizacionydocumentacion"/>
  </patternset>
  
  <path id="optimizacionydocumentacion.module.sourcepath">
    <dirset dir="${module.optimizacionydocumentacion.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.optimizacionydocumentacion" depends="compile.module.optimizacionydocumentacion.production,compile.module.optimizacionydocumentacion.tests" description="Compile module OptimizacionYDocumentacion"/>
  
  <target name="compile.module.optimizacionydocumentacion.production" depends="register.custom.compilers" description="Compile module OptimizacionYDocumentacion; production classes">
    <mkdir dir="${optimizacionydocumentacion.output.dir}"/>
    <javac2 destdir="${optimizacionydocumentacion.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.optimizacionydocumentacion}/javac">
      <compilerarg line="${compiler.args.optimizacionydocumentacion}"/>
      <bootclasspath refid="optimizacionydocumentacion.module.bootclasspath"/>
      <classpath refid="optimizacionydocumentacion.module.production.classpath"/>
      <src refid="optimizacionydocumentacion.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.optimizacionydocumentacion"/>
    </javac2>
    
    <copy todir="${optimizacionydocumentacion.output.dir}">
      <fileset dir="${module.optimizacionydocumentacion.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.optimizacionydocumentacion.tests" depends="register.custom.compilers,compile.module.optimizacionydocumentacion.production" description="compile module OptimizacionYDocumentacion; test classes" unless="skip.tests"/>
  
  <target name="clean.module.optimizacionydocumentacion" description="cleanup module">
    <delete dir="${optimizacionydocumentacion.output.dir}"/>
    <delete dir="${optimizacionydocumentacion.testoutput.dir}"/>
  </target>
</project>